<% content_for :page_title, "#{ "Latest" }" %>

<div class="heading" role="region" aria-labelledby="latest">
  <h1 id="latest">
    <% if params[:page] %>
      <strong>Latest</strong> Codes
      <%= tag.small "- Page #{ params[:page] }" if params[:page].to_i > 1 %>
    <% else %>
      <strong>Latest</strong> Codes
    <% end %>
  </h1>
</div>

<%= render "posts", labelledby: "latest" %>
